{
    "name": "pangpondpon/tswink",
    "description": "Generate typescript classes from Laravel models.",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "illuminate/support": "^10.0||^11.0",
        "illuminate/database": "^10.0||^11.0",
        "doctrine/dbal": "^3.6||^4",
        "phpunit/phpunit": "^7.0||^8.0||^9.3.3||^11.0",
        "illuminate/console": "^10.0||^11.0"
    },
    "author": {
        "name": "Patompong Savaengsuk ",
        "email": "p.pangpond@windowslive.com"
    },
    "autoload": {
        "psr-4": {
            "TsWink\\": "src/",
            "TsWinkTests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TsWink\\TswinkServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpstan/phpstan": "2.0.x-dev"
    }
}
